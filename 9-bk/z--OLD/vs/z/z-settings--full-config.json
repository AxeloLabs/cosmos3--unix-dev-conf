{
  // 2 START
  "workbench.list.smoothScrolling": true,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.iconTheme": null,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook",
    "*.md": "vscode.markdown.preview.editor"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "never",
  "security.workspace.trust.untrustedFiles": "open",
  "zenMode.centerLayout": false,
  "extensions.ignoreRecommendations": true,
  // 3 EDITOR
  "editor.accessibilitySupport": "off",
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": true,
  "editor.suggest.showIcons": false,
  "editor.hover.enabled": false,
  // "editor.fontFamily"                                 : "Cascadia Code",
  // "editor.fontFamily"                                 : "Dank Mono, FuraCode Nerd Font, Inconsolata",
  // "editor.fontFamily"                                 : "flottflott",
  // "editor.fontFamily"                                 : "FuraCode Nerd Font",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.smoothScrolling": true,
  "editor.cursorSurroundingLines": 5,

  "editor.fontSize": 14,
  "editor.cursorStyle": "line",
  "editor.suggestSelection": "first",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.minimap.side": "right",
  "editor.minimap.maxColumn": 80,
  "editor.minimap.scale": 3,
  "editor.minimap.size": "fit",

  "editor.multiCursorModifier": "ctrlCmd",
  "editor.tabCompletion": "on",
  // "editor.autoIndent"                                 : "full",
  // "editor.cursorBlinking"                             : "solid",
  // "editor.cursorSmoothCaretAnimation"                 : true,
  // "editor.cursorStyle"                                : "line",
  // "editor.fontFamily"                                 : "Dank Mono, Fira Code  ,Inconsolata",
  // "editor.formatOnPaste"                              : true,
  // "editor.formatOnType"                               : false,
  // "editor.letterSpacing"                              : 0.3,
  // "editor.lineHeight"                                 : 25,
  // "editor.wordWrap": "on",
  // 3 EDITOR SUGGESTION
  "editor.suggest.showConstants": false,
  "editor.suggest.showEvents": false,
  "editor.suggest.showIssues": false,
  "editor.suggest.showKeywords": false,
  "editor.suggest.showOperators": false,
  "editor.suggest.showUsers": false,
  // 2 TERMINAL
  // "terminal.integrated.env.osx": {
  //   "FIG_NEW_SESSION": "1"
  // },
  "terminal.integrated.fontFamily": "Firicico",
  "terminal.integrated.fontSize": 14,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.cursorBlinking": false,
  "terminal.integrated.scrollback": 4000,
  // "terminal.integrated.shell.osx"                     : "/bin/zsh",
  // "terminal.integrated.fontFamily"                    : "FuraCode Nerd Font",
