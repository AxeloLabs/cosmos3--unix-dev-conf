  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment.block.svelte",
          "comment.line.double-slash.js",
          "comment.line.double-slash.ts",
          "comment.block.html",
          "comment.line.number-sign.shell",
          "comment.block.documentation",
          "storage.type.class.jsdoc",
          "comment.block.documentation.js",
          "variable.other.jsdoc"
        ],
        "settings": {
          "foreground": "#f453a3"
        }
      },
      {
        "scope": [
          "storage.type.js",
          "keyword.control.conditional.js",
          "keyword.control.export.js",
          "keyword.control.import.js",
          "meta.brace.round.js",
          "punctuation.definition.block.js",
          "keyword.operator.assignment.js",
          "punctuation.separator.key-value.js",
          "keyword.operator.spread.js",
          "keyword.control.flow.js",
          "keyword.control.from.js",
          "storage.modifier.async.js",
          "punctuation.definition.template-expression.begin.js",
          "entity.other.attribute-name.svelte",
          "variable.other.readwrite.alias.js",
          "meta.import.js",
          "meta.import.ts",
          "meta.script.svelte entity.name.tag.svelte"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["support.type.property-name.css.postcss"],
        "settings": {
          "foreground": "#d3b416"
        }
      },
      {
        "scope": [
          "support.class.component.svelte",
          "entity.other.attribute-name.class.css.postcss",
          "entity.name.type.class.ts",
          "entity.name.type.class.js"
        ],
        "settings": {
          "foreground": "#01c4da"
        }
      },
      {
        "scope": [
          "storage.type.js",
          "storage.type.ts",
          "support.type.property-name.css.postcss",
          "entity.name.tag.svelte"
        ],
        "settings": {
          "fontStyle": "underline"
        }
      },
      {
        "scope": [
          "entity.name.function.js",
          "entity.other.attribute-name.class.css.postcss",
          "entity.name.type.class.ts",
          "entity.name.type.class.js",
          "support.class.component.svelte"
        ],
        "settings": {
          "fontStyle": "bold"
        }
      }
    ]
  },
