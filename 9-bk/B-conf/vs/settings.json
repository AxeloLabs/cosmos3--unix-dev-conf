{
  // -1
  // -1 WORKBENCH
  // -1
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#41f110",
    "scrollbarSlider.background": "#ff000058",
    "editorLineNumber.activeForeground": "#41f110",

    // -1 tab
    "breadcrumb.background": "#FFF",
    "breadcrumb.foreground": "#000",
    // "editorGroupHeader.tabsBackground": "#fff",
    "tab.activeBorderTop": "#f8e405",
    "tab.activeForeground": "#000",
    "tab.activeBackground": "#fff",
    // "tab.inactiveBackground": "#fff",
    // "tab.inactiveForeground": "#000",
    // "terminal.background": "#090300",
    // "terminal.foreground": "#A5A2A2",
    // "editorInlayHint.background": "#01A0E4",
    // "editorInlayHint.foreground": "#090300",
    "list.inactiveSelectionBackground": "#fff",
    "list.inactiveSelectionForeground": "#000",
    "list.hoverBackground": "#aaaaaa",
    "list.hoverForeground": "#000000",
    "list.errorForeground": "#ff0000",
    "list.warningForeground": "#c5be05",
    // -1
    // -1
    // -1 git - files
    // "gitDecoration.addedResourceForeground": "#00f",
    // "gitDecoration.stageModifiedResourceForeground": "#15d01579",
    "gitDecoration.modifiedResourceForeground": "#30b930",
    // "gitDecoration.renamedResourceForeground": "#269069",
    "gitDecoration.deletedResourceForeground": "#0040ff",
    // "gitDecoration.untrackedResourceForeground": "#fdfdfd",
    // "gitDecoration.ignoredResourceForeground": "#ff0000",
    // "gitDecoration.conflictingResourceForeground": "#00e5ff"
    // -1 Gutter indicators (right)
    // "editorOverviewRuler.addedForeground": "#00f",
    "editorOverviewRuler.modifiedForeground": "#30b930",
    "editorOverviewRuler.deletedForeground": "#0040ff",
    // -1 git - gutter -- // Scrollbar indicators (left + right)
    "editorGutter.modifiedBackground": "#00fd00",
    // "editorGutter.addedBackground": "#fdfdfd",
    "editorGutter.deletedBackground": "#0040ff",

    // -1
    // -1
    // -1 ctrl + P
    "quickInput.foreground": "#000",
    "quickInput.background": "#fff",
    "quickInputList.focusBackground": "#000",
    "quickInputList.focusForeground": "#FFF",
    // this color is the letter matched !
    "list.highlightForeground": "#ff0000",

    // -1 suggest
    "editorSuggestWidget.selectedBackground": "#000",
    "editorSuggestWidget.border": "#fff",
    "editorSuggestWidget.foreground": "#000",
    "editorSuggestWidget.background": "#fff",
    "editorSuggestWidget.selectedForeground": "#fff",
    "editorSuggestWidget.focusHighlightForeground": "#ff0000",
    "editorSuggestWidget.highlightForeground": "#ff0000",

    // -1 terminal
    "terminal.fontLigatures": true
    // "terminal.fontFamily": "Victor Mono",
    // -1 Z
    // "editor.background": "#1a2801"
  },
  // -0
  // -0 Z
  // -0
  // -0 FONT
  "editor.fontLigatures": true,
  "editor.fontFamily": "Victor Mono",
  // "editor.fontFamily": "Mononoki",
  // "editor.fontFamily": "Firicico",
  // "editor.fontFamily": "Cascadia Code",
  // "editor.fontFamily": "FiraCode",
  // -2
  // -2
  // -2 FORMAT - this prettier default formater -> it will remove trailing space
  "svelte.ask-to-enable-ts-plugin": false,

  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    // "source.fixAll": true,
    "source.organizeImports": true,
    "source.sortMembers": true
  },
  // -- svelte for vscode plugin
  "svelte.enable-ts-plugin": true,
  "[svelte]": {
    // -- svelte for vscode plugin
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  // -2
  // -2
  // -2
  // -0 EDITOR
  "trailing-spaces.backgroundColor": "rgba(0,255,0,0.3)",
  "explorer.confirmDragAndDrop": false,
  "editor.suggest.showWords": false, // ne pas afficher suggestion abc
  "editor.tabSize": 2,
  "editor.rulers": [60],
  "workbench.list.horizontalScrolling": true,
  "workbench.editor.labelFormat": "short",
  "security.workspace.trust.untrustedFiles": "open",
  "explorer.confirmDelete": false,
  "workbench.tree.indent": 16,
  "material-icon-theme.folders.color": "#43a047",
  "material-icon-theme.folders.theme": "classic",
  "workbench.iconTheme": "material-icon-theme",
  "editor.cursorBlinking": "solid",
  "workbench.colorTheme": "Darkside (rainglow)",
  // -0 HINT
  "javascript.inlayHints.variableTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  // -0 minimap S
  // "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  // -0 Z
  // "tabnine.experimentalAutoImports": true,

  "search.showLineNumbers": true,
  "search.sortOrder": "fileNames",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "sonarlint.rules": {
    "typescript:S101": {
      "level": "off"
    }
  },
  "[properties]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "files.associations": {
    ".env*": "dotenv"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "keyword.other.dotenv",
        "settings": {
          "foreground": "#FF000000"
        }
      }
    ]
  },
  "[dotenv]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "window.zoomLevel": 1,
  "todohighlight.keywords": [
    {
      "text": "-0",
      "color": "white",
      "backgroundColor": "green",
      "isWholeLine": true,
      "overviewRulerColor": "transparent"
    },

    {
      "text": "\"@axelox/utils\": \"workspace:*\"",
      "color": "white",
      "backgroundColor": "DeepPink",
      "isWholeLine": false,
      "overviewRulerColor": "transparent"
    },
    {
      "text": "\"@axelox/types-blog--toucherterre\": \"workspace:*\"",
      "color": "white",
      "backgroundColor": "DeepPink",
      "isWholeLine": false,
      "overviewRulerColor": "transparent"
    },
    {
      "text": "\"eslint-config-axelox\": \"workspace:*\"",
      "color": "white",
      "backgroundColor": "DeepPink",
      "isWholeLine": false,
      "overviewRulerColor": "transparent"
    },
    {
      "text": "\"@axelox/config-ts\": \"workspace:*\"",
      "color": "white",
      "backgroundColor": "DeepPink",
      "isWholeLine": false,
      "overviewRulerColor": "transparent"
    },
    {
      "text": "\"@axelox/config-tailwind\": \"workspace:*\"",
      "color": "white",
      "backgroundColor": "DeepPink",
      "isWholeLine": false,
      "overviewRulerColor": "transparent"
    },

    {
      "text": "\"@axelox/uiblog\": \"workspace:*\"",
      "color": "white",
      "backgroundColor": "DeepPink",
      "isWholeLine": false,
      "overviewRulerColor": "transparent"
    },
    {
      "text": "\"@axelox/repository-firebase\": \"workspace:*\"",
      "color": "white",
      "backgroundColor": "DeepPink",
      "isWholeLine": false,
      "overviewRulerColor": "transparent"
    }
  ]
}
